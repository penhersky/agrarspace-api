{
  "extends": ["airbnb-typescript/base", "prettier"],
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "project": ["./tsconfig.json", "./**/tsconfig.json"]
  },
  "plugins": ["eslint-plugin-import", "prettier"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "implicit-arrow-linebreak": "off"
  }
}
